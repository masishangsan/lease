page{
  background-color: #EBEBEB;
  font-family: 微软雅黑;
}
.indents{
  display: flex;
  height: 150rpx ;
  align-items: center;
}

.item image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 25rpx;
}

.item{
  margin-top: 20rpx;
  background-color: #fff;
  padding: 4rpx 10rpx;
  
}
.state{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
} 
.state view:nth-child(1){
  font-size: 30rpx;
}
.state view:nth-child(1) text{
  color: red;
}
.state view:nth-child(2),.state view:nth-child(3){
  font-size: 25rpx;
  color: #C1C1C1;
}
.peome{
  background-color: #fff;
  margin-top: 20rpx;
}
.peome image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 25rpx;
}
.tapeo{
  padding: 8rpx 16rpx;
  align-items: center; 
  display: flex;
  border-bottom: 1px solid #EDEDED;
}
.battle{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
}
.teshu{
  display: flex;
  width: 100%;
  justify-content: flex-start;
}
.teshu view:nth-child(1){
  margin-right: 20rpx;
}
.money{
  background-color: #fff;
  margin-top: 20rpx;
  padding: 8rpx 16rpx;
  font-size: 30rpx;
}
.up{
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-bottom:10rpx; 
  border-bottom: 1px solid #EDEDED;
}
.up>view:nth-child(1){
  display: flex;
  justify-content: space-between;
}
.up>view:nth-child(2){
  display: flex;
  justify-content: space-between;
}
.up>view:nth-child(3){
  display: flex;
  justify-content: space-between;
}
.total{
  display: flex;
  height: 50rpx;
  align-items: center;
  justify-content: space-between;
}
.pledge{
  display: flex;
  margin-top: 30rpx;
  justify-content: space-between;
  line-height: 60rpx;
  padding: 8rpx 16rpx;
  background-color: #fff;
  height: 60rpx;
  font-size: 30rpx;
}
.toylistt{
  display: flex;
  margin-top: 30rpx;
  justify-content: space-between;
  line-height: 50rpx;
  padding: 8rpx 16rpx;
  background-color: #fff;
  height: 60rpx;
  font-size: 30rpx;
  box-sizing: border-box;
  border-left:  10rpx solid #fed74b;
}
.indent{
  background-color: #fff;
  padding: 0.2rem 0;
}
.nam{
  font-size: 0.8rem;
  color: #4D4D4D;
  width: 350rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.good_mes{
  display: flex;
  /* justify-content: space-between; */
  padding: 0.4rem 0.5rem;
  background-color: #F5F5F5;
  border-bottom: 1px solid #D6D6D6;
}
.good_mes:last-child{
  border: 0;
}
.nsa{
  display: flex;
  width: 100%;
  justify-content: space-between;
}

.sa{
  height: 2rem;
  align-items: center;
  display: flex;
  justify-content: space-between;
  width: 7.5rem;
  font-size: 0.6rem;
  color: #CDC5BF;
}
.sa view:nth-child(1){
  font-size: 0.65rem;
  border: 1px solid #CDC9C9;
  line-height: 1rem;
  padding: 0.04rem 0.1rem;
  border-radius: 0.2rem;
}
.prnum{
  text-align: right ;
  display: flex;
  flex-direction: column;
  justify-content: center;
  color: #4D4D4D;
  font-size: 0.8rem;
  /*width: 7.5rem;*/
}
.prnum view:nth-child(1){
  font-size: 0.9rem;
}
.prnum view:nth-child(2){
  font-size: 0.7rem;
}
.toylist{
  padding-bottom: 100rpx;
}
.good_mes image{
  width: 5rem;
  height: 3rem;
  margin-right:1rem;
}
.tabar{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  font-size: 27rpx;
  display: flex;
  height: 80rpx;
  background-color: #fff;
  justify-content: flex-end;
  padding: 10rpx 20rpx;
  align-items: center;
}
.tabar view{
  border-radius: 5rpx;
  padding: 10rpx 30rpx;
}
.tabar view:nth-child(1){
  border: 1px solid #D6D6D6;
  margin-right: 20rpx;
}
.tabar view:nth-child(2){
  background-color: #fed74b;
}



























































































