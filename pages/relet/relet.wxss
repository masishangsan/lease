page{
  font-family: 微软雅黑;
  background-color: #EBEBEB;
}
.good_message{
  padding: 0.2rem 0.5rem;
  background-color: #fff;
}
.tips{
  height: 1.5rem;

  font-size: 0.7rem;
  color: #8E8E8E;
}
.good{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1px solid #EDEDED;
  padding: 0.5rem 0;
}
.left{
  display: flex;
  font-size: 0.8rem;
  width: 11rem;
  justify-content: space-between;
  align-items: center;
}
.left image{
  width: 3rem;
  height: 2.7rem;
}
.right view:nth-child(1){
  font-size: 0.8rem;
  color: red;
}
.right view:nth-child(2){
  color: #AAAAAA;
  font-size: 0.7rem;
}
.good_message{
  margin-bottom: 2rem;
}
.time{
  background-color: #fff;
  padding: 0.2rem 0.5rem;
}
.set image{
  width: 1rem;
  height: 1rem; 
}
.title{
  padding: 0.5rem 0;
  font-size: 0.75rem;
  color: #8E8E8E;
}
.set{
  display: flex;
  justify-content: space-between;
  border-top: 1px solid #EDEDED;
  font-size: 0.8rem;
  padding: 0.5rem 0;
} 
.choose picker{
  display: block;
}
.choose{
  display: flex;
  justify-content: space-between;
}
.bill{
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  width: 100%;
  align-items: center;
  z-index: 999;
}
.bill view:nth-child(1){
  flex: 7;
}
.bill view:nth-child(2){
  flex: 3
}
.toal{
  background-color: #fff;
  padding: 0.5rem;
  text-align: left;
}
.toal view:nth-child(1){
  font-size: 0.9rem;
}
.toal view:nth-child(2){
  font-size: 0.7rem;
  color: #AAAAAA;
}
.confirm{
  text-align: center;
  color: #fff;
  background-color: #fed74b;
  height: 100%;
  padding: 0.89rem;
}


































