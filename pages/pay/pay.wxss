page{
  background-color: #EBEBEB;
  font-family: 微软雅黑;
}
.pay{
  background-color: #fff;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-bottom: 1px solid #EBEBEB;
}
.pay_n{
  font-size: 45rpx;
}
.balance{
  font-size: 30rpx;
  margin-top: 10rpx;
}
.details{
  background-color: #fff;
  padding: 15rpx;
}
.details .item{
  font-size: 30rpx;
  color: #AAAAAA;
  display: flex;
  justify-content: space-between;
}
.item view{
  width: 120rpx;
  text-align: center;
}
.item>view:nth-child(1){
  width: 200rpx;
  text-align: center;
}
.content{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  padding: 10rpx 0;
}
.content view{
  width: 120rpx;
  text-align: center;
}
.content>view:nth-child(1){
  width: 200rpx;
  text-align: center;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.post{
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  
}
.post>view:nth-child(1){
  width: 200rpx;
  text-align: center;
}
.post>view:nth-child(2){
  width: 120rpx;
  text-align: center;
}
.fang{
  height: 80rpx;
  font-size: 35rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
}
.way{
  background-color: #fff;
}
.way image{
  width: 1rem;
  height: 1rem;
}
.way>view:nth-child(1){
  border-bottom: 1px solid #EBEBEB;
}
.way>view{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  padding: 0 20rpx;
  font-size: 30rpx;
}
.btn{
  position: fixed;
  bottom: 40rpx;                                 
  left: 30rpx;
  right: 30rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 15rpx;
  background-color: #fed74b;
}













































